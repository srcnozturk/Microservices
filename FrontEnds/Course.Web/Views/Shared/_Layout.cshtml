<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Course.Web</title>
    <link href="~/lib/bootstrap-5.2.3/css/bootstrap.css" rel="stylesheet" />
    <link href="~/lib/bootstrap-5.2.3/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/lib/bootstrap-5.2.3/css/bootstrap.rtl.css" rel="stylesheet" />
    <link href="~/lib/bootstrap-5.2.3/css/bootstrap.rtl.min.css" rel="stylesheet" />
    <a href="~/lib/bootstrap-5.2.3/css/bootstrap.css.map">~/lib/bootstrap-5.2.3/css/bootstrap.css.map</a>
    <a href="~/lib/bootstrap-5.2.3/css/bootstrap.min.css.map">~/lib/bootstrap-5.2.3/css/bootstrap.min.css.map</a>
    <a href="~/lib/bootstrap-5.2.3/css/bootstrap.rtl.css.map">~/lib/bootstrap-5.2.3/css/bootstrap.rtl.css.map</a>
    <a href="~/lib/bootstrap-5.2.3/css/bootstrap.rtl.min.css.map">~/lib/bootstrap-5.2.3/css/bootstrap.rtl.min.css.map</a>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Course.Web</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - Course.Web - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/bootstrap-5.2.3/js/bootstrap.bundle.js"></script>
    <script src="~/lib/bootstrap-5.2.3/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/bootstrap-5.2.3/js/bootstrap.esm.js"></script>
    <script src="~/lib/bootstrap-5.2.3/js/bootstrap.esm.min.js"></script>
    <script src="~/lib/bootstrap-5.2.3/js/bootstrap.js"></script>
    <script src="~/lib/bootstrap-5.2.3/js/bootstrap.min.js"></script>
    <a href="~/lib/bootstrap-5.2.3/js/bootstrap.bundle.js.map">~/lib/bootstrap-5.2.3/js/bootstrap.bundle.js.map</a>
    <a href="~/lib/bootstrap-5.2.3/js/bootstrap.bundle.min.js.map">~/lib/bootstrap-5.2.3/js/bootstrap.bundle.min.js.map</a>
    <a href="~/lib/bootstrap-5.2.3/js/bootstrap.esm.js.map">~/lib/bootstrap-5.2.3/js/bootstrap.esm.js.map</a>
    <a href="~/lib/bootstrap-5.2.3/js/bootstrap.esm.min.js.map">~/lib/bootstrap-5.2.3/js/bootstrap.esm.min.js.map</a>
    <a href="~/lib/bootstrap-5.2.3/js/bootstrap.js.map">~/lib/bootstrap-5.2.3/js/bootstrap.js.map</a>
    <a href="~/lib/bootstrap-5.2.3/js/bootstrap.min.js.map">~/lib/bootstrap-5.2.3/js/bootstrap.min.js.map</a>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
